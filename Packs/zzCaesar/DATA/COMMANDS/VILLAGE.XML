		execdelay="60000">
		<src obj="BaseVillage" />
	</cmd>
	<cmd name="tribute" priority="3"
		immediate="yes"
		button="states/tribute.bmp"
		key="t"
		rollover="Supply"
		description="Constantly sends resources to another settlement"
		failtext="Click on a townhall, village, outpost or shipyard to start sending resources">
		<cmdtext target="BaseTownhall" text="Click on this Townhall to start sending resources" default-text="" />
		<cmdtext target="BaseVillage" text="Click on this Village to start sending resources" default-text="" />
		<cmdtext target="Outpost" text="Click on this Outpost to start sending resources" default-text="" />
		<cmdtext target="GGuardPost" text="Click on this Guard Post to start sending resources" default-text="" />
		<cmdtext target="TTent" text="Click on this Teuton Tent to start sending resources" default-text="" />
		<cmdtext target="CTent" text="Click on this Numidian Tent to start sending resources" default-text="" />
		<cmdtext target="BaseShipyard" text="Click on this Shipyard to start sending resources" default-text="" />
		<src obj="BaseVillage" />
		<src obj="Outpost" />
		<src obj="BaseShipyard" />
	</cmd>
	<cmd name="tribute_default" priority="3"
		immediate="yes"
		cursor="do_something">
