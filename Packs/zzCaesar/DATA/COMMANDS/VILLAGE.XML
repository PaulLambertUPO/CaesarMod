<commands>
	<cmd name="repair village" priority="1"
		method="repopulate"
		button="actions/repair.bmp"
		key="r"
		queueicon="gameres/icons/upgrades/repair.bmp"
		groupverifier="data/subai/village_repopulate_verify.vs"
