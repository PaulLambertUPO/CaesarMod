	<method sig="repair" vs="data/subai/building_repair.vs"/>
	<method sig="broken" vs="data/subai/object_idle.vs"/>
	<properties maxhealth="5000"/>
	<properties sight="600"/>
	<properties armor_slash="0" armor_pierce="0"/>
	<properties radius="600" selection_radius="600"/>
